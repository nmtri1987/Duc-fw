@{
    ViewBag.Title = "ScheduleTask Import";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Ajax.BeginForm("ScheduleTaskEvt", "ScheduleTask",
                                        new AjaxOptions
                                        {
                                            InsertionMode = InsertionMode.Replace,
                                            HttpMethod = "POST",
                                            UpdateTargetId = "ScheduleTasklist",
                                            OnBegin = "onBegin",
                                            OnComplete = "onComplete",
                                        }, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
{
 <section>
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">@L("ScheduleTask")</h4>
                        <a class="heading-elements-toggle"><i class="fa fa-ellipsis-v font-medium-3"></i></a>
                        @*<div class="heading-elements">
                                <ul class="list-inline mb-0">
                                    <li><a data-action="collapse"><i class="ft-minus"></i></a></li>
                                    <li><a data-action="reload"><i class="ft-rotate-cw"></i></a></li>
                                    <li><a data-action="expand"><i class="ft-maximize"></i></a></li>
                                    <li><a data-action="close"><i class="ft-x"></i></a></li>
                                </ul>
                            </div>*@
                    </div>
                    <div class="card-body collapse in">

                        <div class="card-block card-dashboard">
                            <div class="dt-events-log">
                                ScheduleTask - @L("Import Data")
                            </div>
                            <div class="ibox-content">

                                <div class="m-b-lg">
                                   
                                    <form class="form form-horizontal striped-rows form-bordered">
                                        <div class="form-group row">
                                            <label class="col-md-3 label-control">@L("SelectFile")</label>
                                            <div class="col-md-9">
                                                <label id="projectinput8" class="file center-block">
                                                    <input  type="file" id="importScheduleTaskexcelfile" targetid="#preScheduleTask" name="importScheduleTaskexcelfile" class="fileupload" style="display:none;"
                           link="/ScheduleTask/ImportExcelFile" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" />
                                                     <button class="btn btn-primary SelectFile" type="button" targetid="importScheduleTaskexcelfile">@L("SelectFile")</button>
                                                </label>
                                            </div>
                                        </div>
                                    
                                    </form>
                                    <hr />
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="panel panel-success list-panel" id="list-panel">
                                            <div class="panel-body">
                                                <button id="btnScheduleTaskSave" type="button" class="btn btn-primary btn-sm" link="/ScheduleTask/SaveExcel"><span class="fa fa-save" aria-hidden="true"></span></button>
    <button type="button" class="btn btn-default btn-sm refresh-button"><span class="glyphicon glyphicon-refresh" aria-hidden="true"></span></button>
                                                <hr />
                                                <div>
                                                    <table id="preScheduleTask" class="table table-striped table-bordered" data-url='@Url.Action("GetGata", "ScheduleTask")' savebtn="btnScheduleTaskSave"
                                   arrid="ScheduleTaskarr" key="Id" default-search="" allow-search="1" allow-chk="1" style="width:100%"></table>
                            <input type='hidden' value="['CompanyID','Id','Name','Seconds','Type','Enabled','StopOnError','LeasedByMachineName','LeasedUntilUtc','LastStartUtc','LastEndUtc','LastSuccessUtc','CreatedDate','CreatedUser','ScreenID',]" id='ScheduleTaskarr' />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </section>
    <script src="~/Scripts/model/fnIfind.js"></script>
            
}