@using Biz.CS.Models
@model DNHRoles
@{
    ViewBag.Title = "DNHRoles";
}
@using (Ajax.BeginForm("Create", "DNHRoles", 
    new AjaxOptions{
        InsertionMode = InsertionMode.Replace ,
        HttpMethod = "POST",
		UpdateTargetId ="DNHRoleslist",
        LoadingElementId ="loadingDisplay",
        OnSuccess ="closePopup()",
        OnBegin = "onBegin", 
        OnComplete = "onComplete",
    }, new { @class = "form-horizontal", enctype = "multipart/form-data" })){
                    @Html.ValidationSummary(true)
					 @Html.HiddenFor(m => m.Rolename)
					  
 
 
 
 
 
 

				<div class="card">
					 <div class="card-header">
						<h4 class="card-title" id="from-actions-bottom-right"> @L("DNHRoles")</h4>
						<a class="heading-elements-toggle"><i class="fa fa-ellipsis-v font-medium-3"></i></a>
        				<div class="heading-elements">
		                    <ul class="list-inline mb-0">
	                        <li><a data-action="close"><i class="ft-x"></i></a></li>
	                    </ul>
	                </div>
					<div class="card-body collapse in">
						<div class="card-block">
						<div class="form-body"> 
@Html.EditorFor(model => model.CompanyID, new { htmlAttributes = new { @class = "form-control ", @placeholder=L("CompanyID")} })
               

<div class="row">
  <div class="form-group col-xs-12 mb-2">
    <label >@L("Rolename")</label>
    
      @Html.EditorFor(model => model.Rolename, new { htmlAttributes = new { @class = "form-control",@disabled = ""} })
      @Html.ValidationMessageFor(model => model.Rolename, "", new { @class = "text-danger" })
    
  </div>
</div>
@Html.EditorFor(model => model.ApplicationName, new { htmlAttributes = new { @class = "form-control ", @placeholder=L("ApplicationName")} })
               


@Html.EditorFor(model => model.Descr, new { htmlAttributes = new { @class = "form-control ", @placeholder=L("Descr")} })
               


@Html.EditorFor(model => model.isGuest, new { htmlAttributes = new { @class = "form-control ", @placeholder=L("isGuest")} })
               


@Html.EditorFor(model => model.Createduser, new { htmlAttributes = new { @class = "form-control ", @placeholder=L("Createduser")} })
               


@Html.EditorFor(model => model.CreatedDate, new { htmlAttributes = new { @class = "form-control ", @placeholder=L("CreatedDate")} })
               

</div>
						<div class="form-actions right"> <button type="submit" class="btn btn-primary btn-block" name="btnsave" value="save">@L("SaveChange")</button></div>
						</div>
					</div>
	            </div>
					 </div>

 }
  @Scripts.Render("~/bundles/jqueryval")

  