@{
    ViewBag.Title = "T_COM_Master_Employee Import";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Ajax.BeginForm("T_COM_Master_EmployeeEvt", "T_COM_Master_Employee",
                                        new AjaxOptions
                                        {
                                            InsertionMode = InsertionMode.Replace,
                                            HttpMethod = "POST",
                                            UpdateTargetId = "T_COM_Master_Employeelist",
                                            OnBegin = "onBegin",
                                            OnComplete = "onComplete",
                                        }, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
{
    
    <div class="ibox-title">
        <div class="panel-heading">
          
        </div>
    </div>
    <div class="ibox-content">

        <div class="m-b-lg">


            <div class="form-group">
                <label class="col-sm-2 control-label">@L("SelectFile")</label>
                <div class="col-sm-10">
                    <input type="file" id="importT_COM_Master_Employeeexcelfile" targetid="#preT_COM_Master_Employee" name="importT_COM_Master_Employeeexcelfile" class="fileupload" style="display:none;"
                           link="/T_COM_Master_Employee/ImportExcelFile" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" />
                    <button class="btn btn-primary SelectFile" type="button" targetid="importT_COM_Master_Employeeexcelfile">@L("SelectFile")</button>
                </div>
            </div>
            <hr />
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-primary list-panel" id="list-panel">
                    <div class="panel-heading list-panel-heading" style="height:44px;">
                        <h3 class="panel-title list-panel-title">@L("T_COM_Master_Employee")</h3>
                    </div>
                    <div class="panel-body">
                        <button id="btnT_COM_Master_EmployeeSave" type="button" class="btn btn-primary btn-sm" link="/T_COM_Master_Employee/SaveExcel"><span class="fa fa-save" aria-hidden="true"></span></button>
                        <button type="button" class="btn btn-default btn-sm refresh-button"><span class="glyphicon glyphicon-refresh" aria-hidden="true"></span></button>
                        <hr />
                        <div class="table-responsive">
                            <table id="preT_COM_Master_Employee" class="table table-striped table-bordered" data-url='@Url.Action("GetGata", "T_COM_Master_Employee")' savebtn="btnT_COM_Master_EmployeeSave"
                                   arrid="T_COM_Master_Employeearr" key="EmployeeCode" default-search="" allow-search="1" allow-chk="1" style="width:100%"></table>
                            <input type='hidden' value="['EmployeeCode','EmployeeNo','FirstName_EN','MiddleName_EN','LastName_EN','FirstName_VN','MiddleName_VN','LastName_VN','Email','DomainId','ESSPassword','LocationId','GroupCode','SessionCode','DirectManager','IndirectManager','Gender','CategoryID','WorkingHours','JoinedDate','ProbationDays','BloodGroup','MaritalStatusID','EmergencyContactName','EmergencyContactNo','EmergencyContactRelation','DOB','POB','POBID','NativePlace','NationalityID','IDCardNo','IDDOI','IDPOI','PassportNO','PassportDOI','PassportPOI','PassportDOE','PermanentAddress','TempAddress','HomePhone','HandPhone','PersonalEmail','TaxCode','SocialBookNo','SocialBookDOI','SocialBookStatusID','RegisteredHospitalID','RelatedExperience','Age','EmpSourceID','DistanceToOfficeID','ForeignLanguage','InternationalTravelTo','OtherSkills','SickLeaveDays','SIEligibility','SIStartDate','HIEligibility','HIStartDate','UIEligibility','UIStartDate','PickupArea','HomeCity','HomeCountry','TempCity','TempCountry','AwardsInBosch','EmployeeStatusID','TerminationDecisionNo','TerminationDate','TerminationReasonID','ExitInterViewDate','EmployeeTypeID','EmployeeSubTypeID','EmployeeTypeCode','LabourBookNo','LabourBookDOI','LabourBookPOI','AnnualLeaveDays','AssignmentID','IsActive','CreatedBy','CreatedDate','ModifiedBy','ModifiedDate','ContractManager','SubGroupCode','RegForKiosk','IsPersonCapacity','OverAllExperience1',]" id='T_COM_Master_Employeearr' />
                        </div>
                    </div>
                </div>
            </div>
        </div>
        

    </div>
    <script src="~/Scripts/model/fnIfind.js"></script>
            
}