@{
    ViewBag.Title = "ls_PayrollDOWS_RBVH Import";
    Layout = "~/Views/Shared/_Layout.cshtml";
}
@using (Ajax.BeginForm("ls_PayrollDOWS_RBVHEvt", "ls_PayrollDOWS_RBVH",
                                        new AjaxOptions
                                        {
                                            InsertionMode = InsertionMode.Replace,
                                            HttpMethod = "POST",
                                            UpdateTargetId = "ls_PayrollDOWS_RBVHlist",
                                            OnBegin = "onBegin",
                                            OnComplete = "onComplete",
                                        }, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
{
    
    <div class="ibox-title">
        <div class="panel-heading">
          
        </div>
    </div>
    <div class="ibox-content">

        <div class="m-b-lg">


            <div class="form-group">
                <label class="col-sm-2 control-label">@L("SelectFile")</label>
                <div class="col-sm-10">
                    <input type="file" id="importls_PayrollDOWS_RBVHexcelfile" targetid="#prels_PayrollDOWS_RBVH" name="importls_PayrollDOWS_RBVHexcelfile" class="fileupload" style="display:none;"
                           link="/ls_PayrollDOWS_RBVH/ImportExcelFile" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" />
                    <button class="btn btn-primary SelectFile" type="button" targetid="importls_PayrollDOWS_RBVHexcelfile">@L("SelectFile")</button>
                </div>
            </div>
            <hr />
        </div>
        <div class="row">
            <div class="col-md-12">
                <div class="panel panel-primary list-panel" id="list-panel">
                    <div class="panel-heading list-panel-heading" style="height:44px;">
                        <h3 class="panel-title list-panel-title">@L("ls_PayrollDOWS_RBVH")</h3>
                    </div>
                    <div class="panel-body">
                        <button id="btnls_PayrollDOWS_RBVHSave" type="button" class="btn btn-primary btn-sm" link="/ls_PayrollDOWS_RBVH/SaveExcel"><span class="fa fa-save" aria-hidden="true"></span></button>
                        <button type="button" class="btn btn-default btn-sm refresh-button"><span class="glyphicon glyphicon-refresh" aria-hidden="true"></span></button>
                        <hr />
                        <div class="table-responsive">
                            <table id="prels_PayrollDOWS_RBVH" class="table table-striped table-bordered" data-url='@Url.Action("GetGata", "ls_PayrollDOWS_RBVH")' savebtn="btnls_PayrollDOWS_RBVHSave"
                                   arrid="ls_PayrollDOWS_RBVHarr" key="ENtityID" default-search="" allow-search="1" allow-chk="1" style="width:100%"></table>
                            <input type='hidden' value="['Dow_ID','Dow_Code','Beg_Day','End_Day','Dow_Num','ENtityID',]" id='ls_PayrollDOWS_RBVHarr' />
                        </div>
                    </div>
                </div>
            </div>
        </div>
        

    </div>
    <script src="~/Scripts/model/fnIfind.js"></script>
            
}