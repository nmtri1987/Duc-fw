@{
    ViewBag.Title = "Contract Import";
    if (!QueryStringBool("ajax"))
    {
        Layout = LayoutPage;
    }
}
@using (Ajax.BeginForm("T_CMS_Master_ContractEvt", "T_CMS_Master_Contract",
                                                                                        new AjaxOptions
                                                                                        {
                                                                                            InsertionMode = InsertionMode.Replace,
                                                                                            HttpMethod = "POST",
                                                                                            UpdateTargetId = "T_CMS_Master_Contractlist",
                                                                                            OnBegin = "onBegin",
                                                                                            OnComplete = "onComplete",
                                                                                        }, new { @class = "form-horizontal", enctype = "multipart/form-data" }))
{

    @*<div class="ibox-title">
            <div class="panel-heading">

            </div>
        </div>*@
    <section>
        <div class="row">
            <div class="col-md-12">
                <div class="card">
                    <div class="card-header">
                        <h4 class="card-title">@L("Contract")</h4>
                        <a class="heading-elements-toggle"><i class="fa fa-ellipsis-v font-medium-3"></i></a>
                        @*<div class="heading-elements">
                                <ul class="list-inline mb-0">
                                    <li><a data-action="collapse"><i class="ft-minus"></i></a></li>
                                    <li><a data-action="reload"><i class="ft-rotate-cw"></i></a></li>
                                    <li><a data-action="expand"><i class="ft-maximize"></i></a></li>
                                    <li><a data-action="close"><i class="ft-x"></i></a></li>
                                </ul>
                            </div>*@
                    </div>
                    <div class="card-body collapse in">

                        <div class="card-block card-dashboard">
                            <div class="dt-events-log">
                                Contract - Mass Upload
                            </div>
                            <div class="ibox-content">

                                <div class="m-b-lg">
                                   
                                    <form class="form form-horizontal striped-rows form-bordered">
                                        <div class="form-group row">
                                            
                                            <div class="col-md-12">
                                                <div class="pull-right">
                                                    <a id="btnT_CMS_Master_ContractSave" type="button" class="btn btn-success " href='@Url.Action("ExportErrorList","Contract")'><span class="fa fa-download" aria-hidden="true"></span></a>
                                                </div>
                                                <label id="projectinput8" class="file center-block">
                                                    <input type="file" id="importT_CMS_Master_Contractexcelfile" targetid="#preT_CMS_Master_Contract" name="importT_CMS_Master_Contractexcelfile"
                                                           class="fileupload" style="display:none;"
                                                           link="/Contract/ImportExcelFile" accept="application/vnd.openxmlformats-officedocument.spreadsheetml.sheet, application/vnd.ms-excel" />
                                                    <button class="btn btn-info SelectFile" type="button" targetid="importT_CMS_Master_Contractexcelfile">@L("SelectFile")</button>
                                                </label>
                                            </div>
                                        </div>
                                    
                                    </form>
                                    <hr />
                                </div>
                                <div class="row">
                                    <div class="col-md-12">
                                        <div class="panel panel-success list-panel" id="list-panel">
                                            <div class="panel-body">
                                                
                                                
    
                                                <hr />
                                                <div>
                                                    <table id="preT_CMS_Master_Contract" class="table table-striped table-bordered" data-url='@Url.Action("GetGata", "Contract")' savebtn="btnT_CMS_Master_ContractSave"
                                                           arrid="T_CMS_Master_Contractarr" key="ID" default-search="" allow-search="1" allow-chk="1" style="width:100%"></table>
                                                    <input type='hidden' value="['ErrorMesssage','ID','DepartCD','SalutationCD','FirstName_EN','MiddleName_EN','LastName_EN','FirstName_VN','MiddleName_VN',
                                   'LastName_VN','GradeName','PostionName','HomeCountryCurrency','Grossoffer','Joiningdate','Enddate','LocationCD','WorkHoursCD','ContractTermCD','ProbationsPeriodCD','DOB','POB','IDCardNo',
                                   'IDDOI','IDPOICD','HighestDegree','PerAddress']" id='T_CMS_Master_Contractarr' />
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>

                    </div>
                </div>
            </div>
        </div>
    </section>

    <script src="~/Scripts/model/fnIfind.js"></script>

}
